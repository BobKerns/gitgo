{
    "python.analysis.typeCheckingMode": "basic",
    "cSpell.words": [
        "gitgo",
        "worktree",
        "worktrees",
        "gitobj",
        "objectstore"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.crswap": true,
        ".vscode": true,
        "**/__init__.py": true
    },
    "hide-files.files": [],
    "workspace.isHidden": true,
    "explorerExclude.backup": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.crswap": true,
        ".vscode": true,
        "**/__init__.py": true
    }
}